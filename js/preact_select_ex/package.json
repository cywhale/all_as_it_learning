    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.3.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "node-sass": "^4.14.1",
    "preact-cli": "^3.0.0",
    "sass-loader": "^10.0.3",
    "sirv-cli": "1.0.3",
    "style-loader": "^1.3.0",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "nanoid": "^3.1.12",
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1",
    "react-dropdown-tree-select": "^2.5.1",
